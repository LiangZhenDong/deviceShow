<resources>

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="Theme.AppCompat.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
  <style name="ButtonStyle">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textColor">@color/buttonTextColor</item>
        <item name="android:background">@drawable/btn_bg_style</item>
        <item name="android:layout_width">90.0dip</item>
        <item name="android:layout_height">42.0dip</item>
    </style>
    <style name="EditTextStyle">
        <item name="android:textSize">14.0sp</item>
        <item name="android:paddingLeft">5.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">50.0dip</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="DialogTitleImageStyle">
        <item name="android:paddingRight">8.0dip</item>
        <item name="android:layout_width">wrap_content</item>
