    <style name="BaseCheckbox" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">@color/deep_gray</item>
        <item name="android:button">@drawable/base_checkbox_style</item>
    </style>
    <style name="CommonTextBlackMedium" parent="@android:style/Theme">
