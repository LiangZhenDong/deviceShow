        <item name="android:layout_height">48.0dip</item>
        <item name="android:layout_marginLeft">30.0dip</item>
        <item name="android:layout_marginTop">20.0dip</item>
        <item name="android:layout_marginRight">15.0dip</item>
        <item name="android:layout_marginBottom">20.0dip</item>
