        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="TimePickerLayoutStyle">
        <item name="android:gravity">center</item>
        <item name="android:orientation">vertical</item>
        <item name="android:background">@color/contentBackgroundColor</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:minHeight">183.0dip</item>
    </style>
    <style name="TimePickerStyle">
        <item name="android:layout_gravity">center</item>
        <item name="android:padding">15.0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="DialogStyle">
        <item name="android:orientation">vertical</item>
        <item name="android:layout_width">350.0dip</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="LoginGetImeiButtonStyle">
        <item name="android:layout_marginLeft">70.0dip</item>
    </style>
    <style name="ListItem">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:minHeight">50.0dip</item>
    </style>
    <style name="MyCheckBox" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:button">@drawable/checkbox_bg</item>
    </style>
    <style name="spinnerdialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>
    <style name="CancelButtonStyle">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textColor">@color/buttonTextColor</item>
