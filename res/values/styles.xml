        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">5.0dip</item>
        <item name="android:layout_marginTop">20.0dip</item>
        <item name="android:layout_marginRight">20.0dip</item>
        <item name="android:layout_marginBottom">20.0dip</item>
    </style>
    <style name="DatePickerLayoutStyle">
        <item name="android:gravity">center</item>
        <item name="android:orientation">vertical</item>
