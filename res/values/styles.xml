    <style name="ProgressLayoutStyle">
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:minWidth">200.0dip</item>
    </style>
    <style name="ProcessBarStyle">
